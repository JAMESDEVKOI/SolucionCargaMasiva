{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=bulkload_db;Username=postgres;Password=postgres123"
  },
  "RabbitMQSettings": {
    "Host": "localhost",
    "Port": 5672,
    "Username": "guest",
    "Password": "guest",
    "CargaMasivaQueue": "carga_masiva",
    "NotificacionesQueue": "notificaciones",
    "CargaMasivaExchange": "carga_masiva_exchange",
    "NotificacionesExchange": "notificaciones_exchange"
  },
  "SeaweedFSSettings": {
    "MasterUrl": "http://localhost:9333"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}
